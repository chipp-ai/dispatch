{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "title": "Error + Warn Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 0 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "sum by (level) (count_over_time({app=\"chipp-deno\", level=~\"error|warn\"} [1m]))",
          "legendFormat": "{{ level }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "fillOpacity": 20,
            "lineWidth": 2,
            "drawStyle": "line",
            "pointSize": 5,
            "showPoints": "auto"
          },
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "error" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "warn" },
            "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }]
          }
        ]
      }
    },
    {
      "title": "Errors by Source (5m buckets)",
      "type": "barchart",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "sum by (source) (count_over_time({app=\"chipp-deno\", level=\"error\"} | json [5m]))",
          "legendFormat": "{{ source }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" } }
      }
    },
    {
      "title": "Top 20 Errors by Feature (1h)",
      "type": "table",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "topk(20, sum by (feature) (count_over_time({app=\"chipp-deno\", level=\"error\"} | json [1h])))",
          "refId": "A",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Value" },
            "properties": [{ "id": "displayName", "value": "Count" }]
          }
        ]
      },
      "options": {
        "sortBy": [{ "displayName": "Count", "desc": true }]
      }
    },
    {
      "title": "Error Rate by Version",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 16 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "sum by (version) (count_over_time({app=\"chipp-deno\", level=\"error\"} | json [5m]))",
          "legendFormat": "{{ version }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "fillOpacity": 30,
            "lineWidth": 1,
            "drawStyle": "bars",
            "stacking": { "mode": "normal" }
          },
          "unit": "short"
        }
      }
    },
    {
      "title": "Recent Errors",
      "type": "logs",
      "gridPos": { "h": 12, "w": 24, "x": 0, "y": 24 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "{app=\"chipp-deno\", level=\"error\"} | json",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": true,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "prettifyLogMessage": true,
        "enableLogDetails": true,
        "sortOrder": "Descending",
        "dedupStrategy": "none"
      }
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "tags": ["chipp-deno", "errors", "sentry-replacement"],
  "templating": { "list": [] },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Error Overview",
  "uid": "chipp-error-overview"
}
