{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "title": "Log Volume by Source",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 0 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "sum by (source) (count_over_time({app=\"chipp-deno\"} | json [5m]))",
          "legendFormat": "{{ source }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "fillOpacity": 40,
            "lineWidth": 1,
            "drawStyle": "line",
            "stacking": { "mode": "normal" }
          },
          "unit": "short"
        }
      }
    },
    {
      "title": "Active Features Heatmap (source x feature, 1h)",
      "type": "table",
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 8 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "topk(50, sum by (source, feature) (count_over_time({app=\"chipp-deno\"} | json [1h])))",
          "refId": "A",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Value" },
            "properties": [
              { "id": "displayName", "value": "Count" },
              {
                "id": "custom.cellOptions",
                "value": {
                  "type": "color-background",
                  "mode": "gradient"
                }
              },
              {
                "id": "color",
                "value": { "mode": "continuous-GrYlRd" }
              }
            ]
          }
        ]
      },
      "options": {
        "sortBy": [{ "displayName": "Count", "desc": true }]
      }
    },
    {
      "title": "Integration Channel Activity (5m buckets)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 18 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "sum by (source) (count_over_time({app=\"chipp-deno\"} | json | source=~\"whatsapp.*|slack.*|email.*|voice.*\" [5m]))",
          "legendFormat": "{{ source }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "fillOpacity": 20,
            "lineWidth": 2,
            "drawStyle": "line"
          },
          "unit": "short"
        }
      }
    },
    {
      "title": "Agent Loop Executions (5m)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 18 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "count_over_time({app=\"chipp-deno\"} | json | source=~\"agent.*|llm.*|chat.*\" [5m])",
          "legendFormat": "{{ source }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "fillOpacity": 15,
            "lineWidth": 2,
            "drawStyle": "line"
          },
          "unit": "short"
        }
      }
    },
    {
      "title": "WebSocket Activity (5m)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 26 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "sum by (feature) (count_over_time({app=\"chipp-deno\"} | json | source=\"websocket\" [5m]))",
          "legendFormat": "{{ feature }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "fillOpacity": 20,
            "lineWidth": 2,
            "drawStyle": "line"
          },
          "unit": "short"
        }
      }
    }
  ],
  "refresh": "1m",
  "schemaVersion": 39,
  "tags": ["chipp-deno", "features", "analytics"],
  "templating": { "list": [] },
  "time": { "from": "now-24h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Feature Adoption",
  "uid": "chipp-feature-adoption"
}
