{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "title": "Webhook Events by Type (5m)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 0 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "sum by (feature) (count_over_time({app=\"chipp-deno\"} | json | source=\"stripe-webhook\" [5m]))",
          "legendFormat": "{{ feature }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "fillOpacity": 20,
            "lineWidth": 2,
            "drawStyle": "line"
          },
          "unit": "short"
        }
      }
    },
    {
      "title": "Payment Failures",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "count_over_time({app=\"chipp-deno\"} | json | source=\"billing\" | feature=\"payment-failed\" [5m])",
          "legendFormat": "payment failures",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "fixedColor": "red", "mode": "fixed" },
          "custom": {
            "fillOpacity": 30,
            "lineWidth": 2,
            "drawStyle": "bars"
          },
          "unit": "short"
        }
      }
    },
    {
      "title": "Disputes Created",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 8 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "count_over_time({app=\"chipp-deno\"} | json | source=\"billing\" | feature=\"dispute-created\" [$__range])",
          "refId": "A",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "fixedColor": "dark-red", "mode": "fixed" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 3 }
            ]
          }
        }
      },
      "options": { "colorMode": "background", "graphMode": "none" }
    },
    {
      "title": "Credit Exhaustion Events",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 8 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "count_over_time({app=\"chipp-deno\"} | json | source=\"billing\" | feature=\"credit-exhaustion\" [$__range])",
          "refId": "A",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "fixedColor": "orange", "mode": "fixed" },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 5 },
              { "color": "red", "value": 20 }
            ]
          }
        }
      },
      "options": { "colorMode": "background", "graphMode": "none" }
    },
    {
      "title": "Subscription Lifecycle (5m)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "sum by (feature) (count_over_time({app=\"chipp-deno\"} | json | source=\"billing\" | feature=~\"subscription.*\" [5m]))",
          "legendFormat": "{{ feature }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "fillOpacity": 20,
            "lineWidth": 2,
            "drawStyle": "line"
          },
          "unit": "short"
        }
      }
    },
    {
      "title": "Churn Events",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "count_over_time({app=\"chipp-deno\"} | json | source=\"billing\" | feature=\"churn\" [5m])",
          "legendFormat": "churn events",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "fixedColor": "dark-orange", "mode": "fixed" },
          "custom": {
            "fillOpacity": 30,
            "lineWidth": 2,
            "drawStyle": "bars"
          },
          "unit": "short"
        }
      }
    },
    {
      "title": "Recent Billing Errors",
      "type": "logs",
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 20 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "{app=\"chipp-deno\", level=\"error\"} | json | source=~\"billing|stripe-webhook\"",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": true,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "prettifyLogMessage": true,
        "enableLogDetails": true,
        "sortOrder": "Descending",
        "dedupStrategy": "none"
      }
    }
  ],
  "refresh": "1m",
  "schemaVersion": 39,
  "tags": ["chipp-deno", "billing", "revenue"],
  "templating": { "list": [] },
  "time": { "from": "now-24h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Billing Health",
  "uid": "chipp-billing-health"
}
